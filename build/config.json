{
  "listeners": [
      {
          "address": "0.0.0.0",
          "port": 3000,
          "https": false
      }
  ],
  "app": {
      "number_of_threads": 16,
      "document_root": "./static",
      "upload_path": "./uploads",
      "file_types": [
          "gif",
          "png",
          "jpg",
          "js",
          "css",
          "html",
          "ico",
          "swf",
          "xap",
          "apk",
          "cur",
          "xml"
      ],
      "debug": true,
      "log": {
          "log_path": "./logs",
          "logfile_base_name": "server",
          "log_size_limit": 100000000,
          "log_level": "DEBUG"
      }
  }
}
